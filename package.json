{
    "name": "grid-playground",
    "version": "1.0.0",
    "description": "CSS grids are here!",
    "main": "index.html",
    "author": "Dave Pisek",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "parcel index.pug"
    },
    "devDependencies": {
        "autoprefixer": "^9.4.7",
        "parcel-bundler": "^1.11.0",
        "postcss-custom-media": "^7.0.7",
        "postcss-custom-properties": "^8.0.9",
        "postcss-import": "^12.0.1",
        "postcss-normalize": "^7.0.1",
        "postcss-preset-env": "^6.5.0",
        "pug": "^2.0.3"
    },
    "browserslist": [
        ">1%",
        "last 4 versions",
        "Firefox ESR",
        "not ie < 9",
        "ie 11"
    ],
    "postcss": {
        "plugins": {
            "postcss-import": true,
            "postcss-normalize": true,
            "postcss-preset-env": true, 
            "postcss-custom-media": true,
            "autoprefixer": {
                "flexbox": "no-2009"
            }
        }
    }
}
